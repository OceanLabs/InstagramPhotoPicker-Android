<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ly.kite.instagramphotopicker" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:label="@string/app_name" >
        <activity
            android:name="ly.kite.instagramphotopicker.InstagramLoginActivity"
            android:label="@string/title_activity_login"
            android:theme="@style/Theme.InstagramPhotoPicker"
            >
        </activity>
        <activity
            android:name="ly.kite.instagramphotopicker.InstagramGalleryActivity"
            android:label="@string/title_activity_instagram_gallery"
            android:theme="@style/Theme.InstagramPhotoPicker"
            >
        </activity>
    </application>

</manifest>
